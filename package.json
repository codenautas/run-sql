{
  "name": "run-sql",
  "description": "run sql sentences from the command line",
  "version": "0.0.2",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "repository": "codenautas/run-sql",
  "license": "MIT",
  "main": "triage-esV.js",
  "files": [ "lib" ],
  "dependencies": {
    "commander": "~2.9.0",
    "es6-shim": "0.35.0",
    "es6-transpiler": "0.7.18",
    "fs-extra": "~0.26.7",
    "fs-promise": "~0.5.0",
    "lodash": "~4.7.0",
    
    "best-globals": "~0.2.4",
    "mini-tools": "~0.2.7",
    "pg-promise-strict": "~0.2.3",
    "promise-plus": "~0.0.6"
  },
  "devDependencies": {
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "postinstall": "node install/transpile.js",
    "example": "node triage-esV.js --dir example --command \"select cbrt(125) as cubic_root_of_125\"",
    "start": "node triage-esV.js"
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true,
    "esversion": 6
  },
  "eslintConfig": {
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "qa-control": {
    "package-version": "0.2.0",
    "coverage": 0,
    "run-in": "server",
    "stability": "extending",
    "type": "cmd-tool"
  }
}
